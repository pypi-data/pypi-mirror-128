{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "starting_location": {
            "type": "object",
            "properties": {
                "level": {
                    "type": "string"
                },
                "actor": {
                    "type": "string"
                }
            },
            "required": [
                "level", "actor"
            ]
        },
        "starting_items": {
            "type": "object"
        }
    },
    "required": [
        "starting_location",
        "starting_items"
    ]
}
