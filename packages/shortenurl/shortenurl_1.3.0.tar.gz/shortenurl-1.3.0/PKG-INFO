        # Example code
        
        tinyurl.create("apiToken", "urlToShorten", "alias", "domain")\
        tinyurl.change("apiToken", "alias", "domain", "newAlias", and "newUrl")\
        tinyurl.update("apiToken", "alias", "domain", "newAlias", "newDomain")
        
